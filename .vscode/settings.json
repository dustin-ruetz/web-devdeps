{
	"eslint.options": {
		"overrideConfigFile": "lib/config/eslint.config.js"
	},
	"files.associations": {
		"**/.githooks/*": "shellscript",
		"**/.githooks/_/husky": "shellscript"
	},
	"material-icon-theme.files.associations": {
		".caches/.prettiercache": "Prettier",
		"*.mock.ts": "Test-ts",
		"semantic-release.config.js": "semantic-release",
		"semantic-release.config.ts": "semantic-release"
	},
	"material-icon-theme.folders.associations": {
		".caches": "Temp",
		".caches/.jestcache": "Test"
	},
	"prettier.configPath": "lib/config/prettier.config.js",
	"prettier.ignorePath": ".gitignore"
}
